<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= post.body %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= post.status %>
  </p>

  <p>
    <strong>Published date:</strong>
    <%= post.published_date %>
  </p>

  <p>
    <strong>Author:</strong>
    <%= post.author %>
  </p>

  <p>
    <strong>Home loan:</strong>
    <%= post.home_loan %>
  </p>

  <p>
    <strong>Savings:</strong>
    <%= post.savings %>
  </p>

  <p>
    <strong>Investments:</strong>
    <%= post.investments %>
  </p>

  <p>
    <strong>Income:</strong>
    <%= post.income %>
  </p>

  <p>
    <strong>Crypto:</strong>
    <%= post.crypto %>
  </p>

</div>
